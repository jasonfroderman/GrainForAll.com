    <!-- Bower Components -->
    <!--[if lt IE 9]>
    <script src="{{assets}}/bower/es5-shim/es5-shim.js"></script>
    <script src="{{assets}}/bower/json3/lib/json3.min.js"></script>
    <![endif]-->
    <script src="{{assets}}/bower/modernizr.js"></script>
    <script src="{{assets}}/bower/jquery.js"></script>
    <script src="{{assets}}/bower/fastclick.js"></script>
    <script src="{{assets}}/bower/jquery.cookie.js"></script>
    <script src="{{assets}}/bower/jquery.placeholder.js"></script>
    <script src="{{assets}}/bower/foundation.js"></script>
    <script src="{{assets}}/bower/jquery.easing.min.js"></script>

    <!-- Nav -->
    <script src="{{assets}}/js/vendor/classie.js"></script>
    <script src="{{assets}}/js/vendor/sidebareffects.js"></script>

    <script src="{{assets}}/js/app.js"></script>
    <script src="{{assets}}/js/components/nav.js"></script>

    <!-- Page Specific CSS -->
    <link rel="stylesheet" href="{{assets}}/css/pages/{{page_css}}" />

    <!-- Page Specific JS -->
    <script src="{{assets}}/js/pages/{{page_js}}"></script>

</body>
</html>
